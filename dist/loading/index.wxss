.air-loading{color:#2d8cf0;vertical-align:middle;text-align:center}.air-loading-fullscreen{z-index:2010;position:fixed;top:0;right:0;bottom:0;left:0;display:flex;align-items:center;justify-content:center}.air-loading-large{width:30px;height:30px}.air-loading-default{width:20px;height:20px}.air-loading-small{width:12px;height:12px}.air-loading .circle{display:inline-block;margin-right:12px;vertical-align:middle;background:0 0;border-radius:50%;border:2px solid #fff;border-color:#2d8cf0 #2d8cf0 #2d8cf0 transparent;animation:loading .8s linear;animation-iteration-count:infinite}@keyframes loading{0%{transform:rotate(0)}100%{transform:rotate(360deg)}}